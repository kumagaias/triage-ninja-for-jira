{
  "mcpServers": {
    "fetch": {
      "command": "uvx",
      "args": [
        "mcp-server-fetch"
      ],
      "env": {},
      "disabled": false,
      "autoApprove": [
        "fetch"
      ]
    },
    "aws-docs": {
      "command": "uvx",
      "args": [
        "awslabs.aws-documentation-mcp-server@latest"
      ],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR"
      },
      "disabled": true,
      "autoApprove": []
    },
    "chrome-devtools": {
      "command": "npx",
      "args": [
        "-y",
        "chrome-devtools-mcp@latest"
      ],
      "disabled": true,
      "autoApprove": []
    },
    "playwright": {
      "command": "npx",
      "args": [
        "-y",
        "@executeautomation/playwright-mcp-server"
      ],
      "disabled": true,
      "autoApprove": []
    },
    "terraform": {
      "command": "uvx",
      "args": [
        "awslabs.terraform-mcp-server@latest"
      ],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR"
      },
      "disabled": true,
      "autoApprove": []
    },
    "github": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-github"
      ],
      "disabled": false,
      "autoApprove": [
        "create_pull_request",
        "update_pull_request",
        "request_copilot_review",
        "merge_pull_request",
        "pull_request_read",
        "list_pull_requests",
        "issue_write",
        "list_commits",
        "get_commit",
        "search_code",
        "add_comment_to_pending_review",
        "add_issue_comment",
        "get_me",
        "search_repositories",
        "create_issue",
        "get_issue",
        "get_pull_request",
        "get_pull_request_comments",
        "get_pull_request_reviews",
        "update_issue"
      ]
    }
  }
}